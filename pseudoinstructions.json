{
    "^\\s*nop\\s*$": "addi x0, x0, 0",
    "^\\s*mv\\s+(?P<rd>\\w+),\\s*(?P<rs>\\w+)\\s*$": "addi {rd}, {rs}, 0",
    "^\\s*not\\s+(?P<rd>\\w+),\\s*(?P<rs>\\w+)\\s*$": "xori {rd}, {rs}, -1",
    "^\\s*neg\\s+(?P<rd>\\w+),\\s*(?P<rs>\\w+)\\s*$": "sub {rd}, x0, {rs}",
    "^\\s*negw\\s+(?P<rd>\\w+),\\s*(?P<rs>\\w+)\\s*$": "subw {rd}, x0, {rs}",
    "^\\s*seqz\\s+(?P<rd>\\w+),\\s*(?P<rs>\\w+)\\s*$": "sltiu {rd}, {rs}, 1",
    "^\\s*snez\\s+(?P<rd>\\w+),\\s*(?P<rs>\\w+)\\s*$": "sltu {rd}, x0, {rs}",
    "^\\s*sltz\\s+(?P<rd>\\w+),\\s*(?P<rs>\\w+)\\s*$": "slt {rd}, {rs}, x0",
    "^\\s*sgtz\\s+(?P<rd>\\w+),\\s*(?P<rs>\\w+)\\s*$": "slt {rd}, x0, {rs}",
    "^\\s*beqz\\s+(?P<rs>\\w+),\\s*(?P<offset>\\w+)\\s*$": "beq {rs}, x0, {offset}",
    "^\\s*bnez\\s+(?P<rs>\\w+),\\s*(?P<offset>\\w+)\\s*$": "bne {rs}, x0, {offset}",
    "^\\s*blez\\s+(?P<rs>\\w+),\\s*(?P<offset>\\w+)\\s*$": "bge x0, {rs}, {offset}",
    "^\\s*bgez\\s+(?P<rs>\\w+),\\s*(?P<offset>\\w+)\\s*$": "bge {rs}, x0, {offset}",
    "^\\s*bltz\\s+(?P<rs>\\w+),\\s*(?P<offset>\\w+)\\s*$": "blt {rs}, x0, {offset}",
    "^\\s*bgtz\\s+(?P<rs>\\w+),\\s*(?P<offset>\\w+)\\s*$": "blt x0, {rs}, {offset}",
    "^\\s*bgt\\s+(?P<rs>\\w+),\\s*(?P<rt>\\w+),\\s*(?P<offset>[a-zA-Z_][a-zA-Z0-9_]*\\s*\\(.*\\)|[a-zA-Z_.][a-zA-Z0-9_]*)\\s*$": "blt {rt}, {rs}, {offset}",
    "^\\s*ble\\s+(?P<rs>\\w+),\\s*(?P<rt>\\w+),\\s*(?P<offset>[a-zA-Z_][a-zA-Z0-9_]*\\s*\\(.*\\)|[a-zA-Z_.][a-zA-Z0-9_]*)\\s*$": "bge {rt}, {rs}, {offset}",
    "^\\s*bgtu\\s+(?P<rs>\\w+),\\s*(?P<rt>\\w+),\\s*(?P<offset>[a-zA-Z_][a-zA-Z0-9_]*\\s*\\(.*\\)|[a-zA-Z_.][a-zA-Z0-9_]*)\\s*$": "bltu {rt}, {rs}, {offset}",
    "^\\s*bleu\\s+(?P<rs>\\w+),\\s*(?P<rt>\\w+),\\s*(?P<offset>[a-zA-Z_][a-zA-Z0-9_]*\\s*\\(.*\\)|[a-zA-Z_.][a-zA-Z0-9_]*)\\s*$": "bgeu {rt}, {rs}, {offset}",
    "^\\s*j\\s+(?P<offset>[a-zA-Z_][a-zA-Z0-9_]*\\s*\\(.*\\)|[a-zA-Z_.][a-zA-Z0-9_]*)\\s*$": "jal x0, {offset}",
    "^\\s*jal\\s+(?P<offset>[a-zA-Z_][a-zA-Z0-9_]*\\s*\\(.*\\)|[a-zA-Z_.][a-zA-Z0-9_]*)\\s*$": "jal x1, {offset}",
    "^\\s*jr\\s+(?P<rs>[a-zA-Z_][a-zA-Z0-9_]*\\s*\\(.*\\)|[a-zA-Z_.][a-zA-Z0-9_]*)\\s*$": "jalr x0, {rs}, 0",
    "^\\s*jalr\\s+(?P<rs>\\w+)\\s*$": "jalr x1, {rs}, 0",
    "^\\s*ret\\s*$": "jalr x0, x1, 0",
    "^\\s*call\\s+(?P<symbol>\\w+)\\s*$": ["auipc x1, {symbol1}", "jalr x1, x1, {symbol2}"],
    "^\\s*tail\\s+(?P<symbol>\\w+)\\s*$": ["auipc x6, {symbol1}", "jalr x0, x6, {symbol2}"],
    "^\\s*li\\s+(?P<rd>\\w+),\\s*(?P<symbol>[+-]?\\d+|0[xX][0-9a-fA-F]+)\\s*$": ["lui {rd}, {symbol1}", "addi {rd}, x0, {symbol2}"],
    "^\\s*l(?P<letter>[bhwd])\\s+(?P<rd>\\w+),\\s*(?P<symbol>[+-]?\\d+|0[xX][0-9a-fA-F]+)\\s*$": ["auipc {rd}, {symbol1}", "l{letter} {rd}, {symbol1}({rd})"],
    "^\\s*s(?P<letter>[bhwd])\\s+(?P<rd>\\w+),\\s*(?P<symbol>[+-]?\\d+|0[xX][0-9a-fA-F]+),\\s*(?P<rt>\\w+)\\s*$": ["auipc {rt}, {symbol1}", "s{letter} {rd}, {symbol2}({rt})"],
    "^la\\s+(?P<rd>\\w+),\\s*(?P<symbol>[+-]?\\d+|0[xX][0-9a-fA-F]+)\\s*$": ["auipc {rd}, {symbol1}", "addi {rd}, {rd}, {symbol2}"]
}